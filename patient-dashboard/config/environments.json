{
  "development": {
    "name": "Development",
    "ports": {
      "frontend": 3000,
      "backend": 8001,
      "database": 8000
    },
    "database": {
      "name": "patient_dashboard_dev",
      "namespace": "patient_dashboard_dev"
    },
    "urls": {
      "frontend": "http://localhost:3000",
      "backend": "http://localhost:8001",
      "database": "ws://localhost:8000/rpc"
    },
    "docker": {
      "compose_file": "docker-compose.dev.yml",
      "network": "pfinni-dev-network",
      "container_prefix": "pfinni-*-dev"
    }
  },
  "production": {
    "name": "Production",
    "ports": {
      "frontend": 3100,
      "backend": 8101,
      "database": 8100
    },
    "database": {
      "name": "patient_dashboard_prod",
      "namespace": "patient_dashboard_prod"
    },
    "urls": {
      "frontend": "https://devq.ai/pfinni",
      "backend": "https://api.devq.ai/pfinni",
      "database": "wss://db.devq.ai/pfinni"
    },
    "docker": {
      "compose_file": "docker-compose.prod.yml",
      "network": "pfinni-prod-network",
      "container_prefix": "pfinni-*-prod"
    }
  }
}