# Production Environment Variables Template
# Copy this to .env.production and fill in actual values

# Production Clerk Keys
CLERK_SECRET_KEY_PROD=<your-production-clerk-secret>
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY_PROD=<your-production-clerk-publishable-key>

# Production Database Credentials
SURREAL_USER=<production-db-user>
SURREAL_PASS=<production-db-password>

# Production Logfire Token
PFINNI_LOGFIRE_TOKEN_PROD=<your-production-logfire-token>

# Production CORS Origins
PRODUCTION_CORS_ORIGINS=https://devq.ai

# Production Ports (different from dev)
PROD_FRONTEND_PORT=3100
PROD_BACKEND_PORT=8101
PROD_DATABASE_PORT=8100

# Production flags
IS_DEVELOPMENT=false
SKIP_AUTH=false
DEBUG_MODE=false